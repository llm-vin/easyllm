{
  "name": "@llmvin/easyllm",
  "version": "0.1.0",
  "description": "A unified SDK for OpenAI-compatible LLM APIs including llm.vin and others",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:unit": "jest --testPathPattern=client.test.ts",
    "test:integration": "jest --testPathPattern=integration.test.ts",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "echo 'No linter configured yet'",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "ai",
    "llm",
    "openai",
    "api",
    "sdk",
    "chat",
    "completions",
    "llm.vin",
    "web-search",
    "function-calling",
    "streaming"
  ],
  "author": "llm.vin",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/llm-vin/easyllm"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "dist/**/*.js.map",
    "dist/**/*.d.ts.map",
    "!dist/**/__tests__/**",
    "README.md"
  ],
  "dependencies": {
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "jest": "^29.0.0",
    "@types/jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "dotenv": "^16.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}